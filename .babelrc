{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ],
        "stage-2",
        "react"
      ]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": false
          }
        ],
        "stage-2",
        "react"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ],
        "stage-2",
        "react"
      ],
      "plugins": ["transform-react-remove-prop-types"]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ],
        "stage-2",
        "react"
      ]
    }
  }
}
